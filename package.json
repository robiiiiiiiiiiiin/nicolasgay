{
  "name": "nicolasgay",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "concurrently \"npm run watch:eleventy\" \"npm run watch:js\" --names \"11ty,esbuild\" --prefix-colors \"blue,green\"",
    "watch:windows": "concurrently \"npm run watch:eleventy-win\" \"npm run watch:js-win\" --names \"11ty,esbuild\" --prefix-colors \"blue,green\"",
    "watch:eleventy": "NODE_ENV=development npx @11ty/eleventy --serve",
    "watch:eleventy-win": "SET NODE_ENV=development & npx @11ty/eleventy --serve",
    "watch:js": "NODE_ENV=development esbuild src/assets/js/main.js --bundle --outfile=_site/js/bundle.js --format=iife --sourcemap --watch",
    "watch:js-win": "SET NODE_ENV=development & esbuild src/assets/js/main.js --bundle --outfile=_site/js/bundle.js --format=iife --sourcemap --watch",
    "build": "npm run build:js && npm run build:eleventy",
    "build:eleventy": "NODE_ENV=production npx @11ty/eleventy",
    "build:js": "NODE_ENV=production esbuild src/assets/js/main.js --bundle --outfile=_site/js/bundle.js --format=iife --minify"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "type": "module",
  "dependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-img": "^6.0.2",
    "@contentful/rich-text-html-renderer": "^17.1.0",
    "@fancyapps/ui": "^6.0.14",
    "contentful": "^11.5.22",
    "dotenv": "^17.2.0",
    "isotope-layout": "^3.0.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.5",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.6",
    "esbuild": "^0.25.6",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.5"
  }
}
